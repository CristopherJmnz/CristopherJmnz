{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.cristopher-jmnz.vercel.app" }],
      "destination": "https://cristopher-jmnz.vercel.app/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
